<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
	<mapper namespace="org.zerock.myapp.persistence.MemberDAO">
		
		<select id="memberLogin" resultType="org.zerock.myapp.domain.MemberVO">
		    SELECT 
		    	*
		    FROM 
		    	member 
		    WHERE 
		        id = #{id} 
		        AND password = #{password}
		</select>
	
	</mapper>

